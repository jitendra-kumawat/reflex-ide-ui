{
  "name": "reflex-ide",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:aot": "ng serve --port 2018 --aot",
    "start": "ng serve --port 2018 --aot=false",
    "build": "ng build",
    "prebuild:prod": "npm rebuild node-sass --force",
    "build:prod": "node --stack_size=4048 node_modules/.bin/ng build --prod --environment=prod --aot",
    "postbuild:prod": "./update-build-info.sh $npm_package_config_version $npm_package_version",
    "docker:build": "npm run build:prod && docker build -t reflex-ide:$npm_package_version .",
    "docker:run": "docker run -it -p 4000:80 --name reflex-ide --rm reflex-ide:$npm_package_version",
    "predocker:run": "(lite-server -c bs-config-docker.js) &",
    "docker:tag": "docker tag reflex-ide artifacts.ggn.in.guavus.com:4245/reflex-ide:$npm_package_version",
    "docker:push": "docker push artifacts.ggn.in.guavus.com:4245/reflex-ide:$npm_package_version",
    "test": "ng test --code-coverage --sourcemaps=false",
    "test:watch": "ng test --code-coverage --browsers Chrome --no-single-run --sourcemaps=false",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e --app=app",
    "e2e:result": "http-server -c-1 -o -p 1999 ./e2e-results/screenshots",
    "doc": "compodoc -p ./apps/app/src/tsconfig.app.json -o -s -a doc-images",
    "prestart:prod": "npm run build:prod && npm run start:proxy",
    "start:proxy": "(lite-server) &",
    "start:prod": "lite-server -c bs-config-prod.js",
    "start:ext": "ng serve  --port 1986 --host  `ifconfig | sed -En 's/127.0.0.1//;s/.*inet (addr:)?(([0-9]*\\.){3}[0-9]*).*/\\2/p'`",
    "preverify": ". ~/.nvm/nvm.sh && nvm use",
    "verify": "npm run test && npm run build:prod && npm run lint",
    "coverage": "http-server -c-1 -o -p 2018 ./coverage/html",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update:check",
    "update:skip": "./node_modules/.bin/nx update:skip",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "@guavus/auth": "^6.2.0",
    "@guavus/auth-interceptor": "^6.2.0",
    "@guavus/core": "^6.2.0",
    "@guavus/jscomponents": "^2.0.0",
    "@guavus/smart-components": "^6.2.0",
    "@guavus/styleguide": "^1.0.14",
    "@ngrx/effects": "5.1.0",
    "@ngrx/router-store": "5.0.1",
    "@ngrx/store": "5.1.0",
    "@ngrx/store-devtools": "5.1.0",
    "@nrwl/nx": "1.0.3",
    "ag-grid": "^14.2.0",
    "ag-grid-angular": "^14.2.0",
    "angular2-virtual-scroll": "0.3.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "jquery": "^3.2.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.3.2",
    "@angular-devkit/core": "0.3.2",
    "@angular-devkit/schematics": "^0.6.8",
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "5.2.7",
    "@angular/language-service": "5.2.7",
    "@compodoc/compodoc": "^1.0.5",
    "@guavus/schematics": "^0.11.3",
    "@nrwl/schematics": "1.0.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.10.2",
    "protractor": "~5.1.2",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  }
}
